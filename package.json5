{
  name: 'gMaelstrom',
  version: '1.0.0',
  private: true,
  type: 'module',
  engines: {
    pnpm: '>=9.0.0', // nugget: makes sure we don't inadvertently use npm or yarn
    npm: '0.0.0-please-use-pnpm',
    node: '>=20.0.0',
  },
  scripts: {
    dev: 'node scripts/copy-readme.js && rsbuild dev --open',
    // prebuild: 'node src/scripts/sass-vars-to-ts.cjs',
    build: 'node scripts/copy-readme.js && rsbuild build',
    check: 'biome check --write',
    format: 'prettier --write .',
    lint: 'eslint .',
    preview: 'rsbuild preview',
  },
  dependencies: {
    '@emotion/react': '^11.14.0',
    '@emotion/styled': '^11.14.0',
    '@mui/icons-material': '^7.0.2',
    '@mui/material': '^7.0.2',
    '@mui/x-data-grid': '^8.22.1',
    'date-fns': '^4.1.0',
    react: '^19.1.0',
    'react-dom': '^19.1.0',
    'react-markdown': '^10.1.0',
    'react-router-dom': '^7.5.2',
    'react-toastify': '^11.0.5',
    'rehype-raw': '^7.0.0',
  },
  devDependencies: {
    '@biomejs/biome': '^1.9.4',
    '@eslint/compat': '^1.2.7',
    '@eslint/js': '^9.23.0',
    '@rsbuild/core': '^1.3.1',
    '@rsbuild/plugin-react': '^1.1.1',
    '@rsbuild/plugin-sass': '^1.3.2',
    '@rsbuild/plugin-svgr': '^1.2.0',
    '@rsbuild/plugin-type-check': '^1.2.1',
    '@types/gapi.client.gmail-v1': '^0.0.4', // gmail api types
    '@types/google.accounts': '^0.0.18', //all the nested types under window.google.accounts namespace
    '@types/react': '^19.1.0',
    '@types/react-dom': '^19.1.1',
    cypress: '^15.8.1',
    dotenv: '^16.5.0',
    eslint: '^9.23.0',
    'eslint-plugin-react': '^7.37.4',
    'eslint-plugin-react-hooks': '^5.2.0',
    globals: '^16.0.0',
    'google-auth-library': '^10.5.0', //this is ONLY to provide type TokenPayload for gAuthApi.tsx
    prettier: '^3.5.3',
    sass: '^1.97.1',
    typescript: '^5.8.2',
    'typescript-eslint': '^8.29.0',
  },
}